{
    "name": "gaia-js-sdk-convey",
    "version": "0.10.0",
    "description": "JavaScript connector to the G.A.I.A. ecosystem.",
    "main": "dist/lib/all.js",
    "dependencies": {
        "d3": "^5.8.0",
        "d3-3d": "^0.0.9",
        "d3-sankey": "^0.7.1",
        "d3-scale": "^2.2.2",
        "d3-time": "^1.0.10",
        "d3-time-format": "^2.1.3",
        "d3-tip": "^0.9.1",
        "google-maps": "^3.3.0",
        "leaflet": "1.3.4",
        "mqtt": "2.18.8",
        "node-ical": "0.9.0"
    },
    "devDependencies": {
        "@types/d3": "^5.7.0",
        "@types/d3-sankey": "^0.7.4",
        "@types/d3-scale": "^2.1.0",
        "@types/d3-time": "^1.0.9",
        "@types/d3-time-format": "^2.1.0",
        "@types/d3-tip": "^3.5.5",
        "@types/google-maps": "^3.2.0",
        "@types/jest": "^23.3.10",
        "@types/leaflet": "^1.2.9",
        "@types/mqtt": "^2.5.0",
        "@types/node": "^10.7.1",
        "@types/sinon": "^7.0.5",
        "awesome-typescript-loader": "^5.2.1",
        "copyfiles": "^2.1.0",
        "css-loader": "^2.1.0",
        "import-glob-loader": "^1.1.0",
        "jest": "^23.6.0",
        "jest-junit": "5.1.0",
        "mini-css-extract-plugin": "^0.4.2",
        "node-sass": "^4.11.0",
        "release-it": "^9.8.3",
        "sass-loader": "^7.1.0",
        "shx": "^0.3.2",
        "sinon": "^7.2.3",
        "sonarqube-scanner": "2.1.2",
        "style-loader": "^0.23.1",
        "ts-jest": "^23.10.5",
        "ts-loader": "^4.5.0",
        "tslint": "^5.11.0",
        "tslint-config-airbnb": "^5.10.0",
        "tslint-loader": "^3.6.0",
        "typescript": "^3.0.1",
        "typings-for-css-modules-loader": "^1.7.0",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.11",
        "webpack-node-externals": "^1.7.2"
    },
    "jest-junit": {
        "suiteName": "Frontend Tests",
        "suiteNameTemplate": "{filename}",
        "output": "./test-results/test/TEST-Frontend.xml",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}"
    },
    "scripts": {
        "copy-scss": "copyfiles -u 1 ./src/**/*.scss ./dist/lib && copyfiles -u 1 ./src/**/*.scss ./dist/lib-esm",
        "webpack:dev": "webpack-dev-server --open --mode=development",
        "webpack:build": "webpack --mode=production",
        "webpack:bundle": "webpack --config webpack.bundle.config.js --mode=production",
        "webpack:dev-build": "webpack --mode=development",
        "bundle": "npm run test && npm run webpack:bundle",
        "clean": "shx rm -rf dist lib lib-esm",
        "build": "tsc && tsc -m es6 --outDir ./dist/lib-esm && npm run webpack:build && npm run copy-scss",
        "lint": "tslint src/**/*.ts",
        "sonarqube": "jest --env=jsdom --coverage && sonar-scanner -Dsonar.host.url=$SONAR_HOST_URL -Dsonar.login=$SONAR_LOGIN",
        "trigger-release:major": "git pull --tags && git tag release-major && git push --tags && git tag -d release-major",
        "cleanup-release:major": "git tag -d release-major && git push -d origin release-major",
        "trigger-release:minor": "git pull --tags && git tag release-minor && git push --tags && git tag -d release-minor",
        "cleanup-release:minor": "git tag -d release-minor && git push -d origin release-minor",
        "trigger-release:patch": "git pull --tags && git tag release-patch && git push --tags && git tag -d release-patch",
        "cleanup-release:patch": "git tag -d release-patch && git push -d origin release-patch",
        "release:major": "release-it major",
        "release:minor": "release-it minor",
        "release:patch": "release-it patch --no-git.requireUpstream",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leftshiftone/gaia-js-sdk-convey.git"
    },
    "keywords": [
        "digital",
        "assistant",
        "bot",
        "gaia",
        "g.a.i.a.",
        "leftshiftone"
    ],
    "author": "Leftshift One Software GmbH <core-devs@leftshift.one>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/leftshiftone/gaia-js-sdk-convey/issues"
    },
    "homepage": "https://github.com/leftshiftone/gaia-js-sdk-convey",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    }
}
